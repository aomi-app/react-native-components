{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "sourceMap": false,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "jsx": "react-native",
    "lib": [
      "es2015",
      "es2015.promise",
      "es2015.reflect",
      "es2015.promise",
      "es2016",
      "es2016.array.include",
      "es2017",
      "dom"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
